myOnto:Person a owl:Class ;
    rdfs:label "Person"@en ;
    rdfs:comment "A class defining a Person."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass dbo:Country ;
            owl:onProperty myOnto:birthCountry ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty myOnto:organizationName ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty myOnto:hasPrizeName ],
        [ a owl:Restriction ;
            owl:maxCardinality "3"^^xsd:nonNegativeInteger ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty myOnto:prizeShare ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty myOnto:birthDate ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty myOnto:laureateID ],
        schema:Person .

