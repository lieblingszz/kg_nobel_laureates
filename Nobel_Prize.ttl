schema:birthPlace a owl:ObjectProperty .

schema:familyName a owl:DatatypeProperty .

schema:givenName a owl:DatatypeProperty .

dbo:NobelPrize a owl:Class .

myOnto:birthCountry a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "Birth Country"@en ;
    rdfs:comment "The country where a person was born."@en ;
    rdfs:domain myOnto:Person ;
    rdfs:range dbo:Country .

myOnto:birthDate a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "Date"@en ;
    rdfs:comment "The date of birth of a person"@en ;
    rdfs:domain myOnto:Person ;
    rdfs:range xsd:date ;
    rdfs:subPropertyOf schema:birthDate .

myOnto:hasPrizeName a owl:DatatypeProperty,
        owl:InverseFunctionalProperty ;
    rdfs:label "Prize Name"@en ;
    rdfs:comment "The name of a Nobel prize category"@en ;
    rdfs:domain myOnto:NobelPrize ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf schema:name .

myOnto:laureateID a owl:DatatypeProperty,
        owl:InverseFunctionalProperty ;
    rdfs:label "Laureate ID"@en ;
    rdfs:comment "The unique identifier assigned to each Nobel laureate."@en ;
    rdfs:domain myOnto:Person ;
    rdfs:range xsd:integer .

myOnto:organizationName a owl:ObjectProperty ;
    rdfs:label "Organization Name"@en ;
    rdfs:comment "The organization which a person belongs to."@en ;
    rdfs:domain myOnto:Person ;
    rdfs:range schema:Organization .

myOnto:prizeShare a owl:DatatypeProperty ;
    rdfs:label "Prize Share"@en ;
    rdfs:comment "The allocation of the Nobel Prize award money among the recipients of a particular Nobel Prize category."@en ;
    rdfs:domain myOnto:Person ;
    rdfs:range xsd:integer .

