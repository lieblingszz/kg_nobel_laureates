@prefix schema: <https://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

myOnto:NobelPrize a rdfs:Class ;
    rdfs:label "Nobel Prize"@en ;
    rdfs:comment "A class defining a Nobel Prize."@en ;
    rdfs:subClassOf dbo:NobelPrize .

myOnto: a owl:Ontology ;
    rdfs:comment "An ontology for Semantic Web course final project" .

myOnto:birthCountryCode a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "Birth Country Code"@en ;
    rdfs:comment "The code of a person's birth country"@en ;
    rdfs:domain myOnto:Person ;
    rdfs:range xsd:string .

myOnto:gender a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "Gender"@en ;
    rdfs:comment "The gender of a person"@en ;
    rdfs:domain myOnto:Person ;
    rdfs:range schema:GenderType ;
    rdfs:subPropertyOf schema:gender .

myOnto:motivation a owl:DatatypeProperty ;
    rdfs:label "Motivation"@en ;
    rdfs:comment "The official statement or explanation for awarding a Nobel Prize to a particular individual or group."@en ;
    rdfs:domain myOnto:Person ;
    rdfs:range xsd:string .

myOnto:prizeCategory a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "Nobel Prize Category"@en ;
    rdfs:comment "The Nobel Prize category of a person."@en ;
    rdfs:domain myOnto:Person ;
    rdfs:range myOnto:NobelPrize .

myOnto:prizeYear a owl:DatatypeProperty ;
    rdfs:label "Prize Year"@en ;
    rdfs:comment "The year a person won a Nobel Prize."@en ;
    rdfs:domain myOnto:Person ;
    rdfs:range xsd:gYear .

